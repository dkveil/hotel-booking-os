{
	"name": "admin",
	"version": "1.0.0",
	"description": "A blank template to get started with Payload 3.0",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "next dev --turbopack --port 5001",
		"kill": "pnpm dlx kill-port 5000",
		"build": "next build",
		"start": "next start",
		"generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
		"generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
		"lint": "biome lint .",
		"format": "biome format . --write",
		"check": "biome check .",
		"typecheck": "tsc --noEmit",
		"format:check": "biome format . --check"
	},
	"dependencies": {
		"@payloadcms/next": "3.54.0",
		"@payloadcms/payload-cloud": "3.54.0",
		"@payloadcms/richtext-lexical": "3.54.0",
		"@payloadcms/ui": "3.54.0",
		"cross-env": "^7.0.3",
		"dotenv": "16.4.7",
		"graphql": "^16.8.1",
		"next": "15.4.4",
		"payload": "3.54.0",
		"react": "19.1.0",
		"sharp": "0.34.2",
		"@payloadcms/db-postgres": "3.54.0"
	},
	"devDependencies": {
		"@repo/tailwind-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@repo/vitest-config": "workspace:*",
		"@repo/playwright-config": "workspace:*",
		"@playwright/test": "^1.54.2",
		"postcss": "^8.5.6",
		"@tailwindcss/postcss": "^4.1.11",
		"tailwindcss": "^4.1.11",
		"@testing-library/react": "16.3.0",
		"@types/node": "^22.5.4",
		"@types/react": "19.1.8",
		"@types/react-dom": "19.1.6",
		"@vitejs/plugin-react": "4.5.2",
		"eslint": "^9.16.0",
		"eslint-config-next": "15.4.4",
		"jsdom": "26.1.0",
		"playwright": "1.54.1",
		"playwright-core": "1.54.1",
		"prettier": "^3.4.2",
		"typescript": "5.7.3",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "^3.2.4",
		"@vitest/coverage-istanbul": "^3.2.4"
	},
	"engines": {
		"node": "^18.20.2 || >=20.9.0",
		"pnpm": "^9 || ^10"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"sharp",
			"esbuild",
			"unrs-resolver"
		]
	}
}
